#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *next;
};

void linked_List_Traverse(struct node *newnode){
    while(newnode!=NULL){
        printf("Element:%d\n",newnode->data);
        newnode=newnode->next;
    }
}

